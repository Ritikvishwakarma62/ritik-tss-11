<%-include("../includes/header")%>
<div class="container my-3" style="min-height: 700px;">
    <div class="row">
        <div class="col-md-8 offset-md-2">
            <h4 class="text-center">List Of All Teachers</h4>
            <table class="table table-dark table-border table-hover table-striped">
             <tr>
                <th>S.no.</th>
                <th>Name</th>
                <th>Salary</th>
                <th>Contact</th>
                <th>Delete</th>
             </tr>
             <% result.forEach((value,index)=>{%>
                <tr>
                    <td><%-index+1%></td>
                    <td><%-value.name%></td>
                    <td><%-value.salary%></td>
                    <td><%-value.contact%></td>
                    <td><a href="/teacher/delete/<%-value._id%>" class="btn btn-danger">Delete</a></td>
                </tr>
             <% })%>
            </table>
        </div>
    </div>
</div>
<%-include("../includes/footer")%>