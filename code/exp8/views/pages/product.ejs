<%-include("../includes/header")%>
<h1>product</h1>

<div class="row">
    <%
    data.forEach((value,index)=>{%>

    <div class="col-md-3">
      <div class="card my-2">
        <img class="card-img-top" style="height: 300px; min-height: 120px;" src="<%-value.image%>" alt="">
        <div class="card-body">
          <h5><%- value.title%></h5>
          <p class="m-0 p-0">$<%-value.price%></p>
          <p><small><%-value.category%></small></p>
        </div>
        <div class="card-footer">
          <button class="btn btn-info">Buy</button>
        </div>
      </div>
    </div>
    <% })%>
  </div>