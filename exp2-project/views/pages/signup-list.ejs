<%-include("../includes/header")%>
<div class="container my-2" style="min-height: 890px;">
    <div class="row">
        <div class="col-md-6 offset-md-3">
            <h2 class="text-center">User-List</h2>
            <table class="table table-warning table-hover">
                <tr>
                    <th>S.No.</th>
                    <th>Name</th>
                    <th>Email</th>
                    <th>Country</th>
                    <th>Edit</th>
                    <th>Delete</th>
                </tr>
                <% result.forEach((value,index)=>{ %>
                  <tr>
                    <td><%-index+1%></td>
                    <td><%-value.name%></td>
                    <td><%-value.email%></td>
                    <td><%-value.country%></td>
                    <td><a href="/signup/edit/<%-value._id%>" class="btn btn-success btn-sm">Edit</a></td>
                    <td><a href="/signup/delete/<%-value._id%>" class="btn btn-danger btn-sm">Delete</a></td>
                  </tr>
               <% }) %>
              </table>
        </div>
    </div>
</div>
<%-include("../includes/footer")%>