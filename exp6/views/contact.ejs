<html>

<head>
    <link type="text/css" rel="stylesheet" href="/css/bootstrap.css">
    <link type="text/css" rel="stylesheet" href="/css/font-awesome.css">
    <script src="/js/bootstrap.bundle.js"></script>
</head>

<body>
    <nav class="navbar bg-dark navbar-dark navbar-expand-sm">
        <div class="container">
            <a href="" class="navbar-brand">
                <h5>Nature</h5>
            </a>
            <button class="navbar-toggler" data-bs-toggle="collapse" data-bs-target="#menu">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div id="menu" class="collapse navbar-collapse">
                <ul class="nav navbar-nav">
                    <li class="nav-item">
                        <a href="/" class="nav-link"><b>Home</b></a>
                    </li>
                    <li class="nav-item">
                        <a href="/about" class="nav-link"><b>About</b></a>
                    </li>
                    <li class="nav-item">
                        <a href="/contact" class="nav-link"><b>Contact</b></a>
                    </li>
                    <li class="nav-item">
                        <a href="/product" class="nav-link"><b>product</b></a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    <h1>contact</h1>
    <div class="container">
        <div class="row">
            <div class="col-md-12">
               <%
               let data =[
                {
                    title: "I-phone",
                    price:25000,
                    category:"mobile",
                    image:""
                },
                {
                    title: "vivo",
                    price:20000,
                    category:"mobile",
                    image:""
                },
                {
                    title: "jens",
                    price:2000,
                    category:"mens fashion",
                    image:""
                },
                {
                    title: "laptop",
                    price:35000,
                    category:"laptop",
                    image:""
                }
               ]
               %>

               <table class="table table-dark table-hover">
                <tr>
                    <th>SNo</th>
                    <th>title</th>
                    <th>price</th>
                    <th>category</th>
                </tr>
                <%
                data.forEach((value,index)=>{%>
                    <tr>
                        <td><%-index+1%></td>
                        <td><%-value.title%></td>
                        <td><%-value.price%></td>
                        <td><%-value.category%></td>
                    </tr>
               <%})
                %>
               </table>
               
             

            </div>
        </div>
    </div>
</body>

</html>